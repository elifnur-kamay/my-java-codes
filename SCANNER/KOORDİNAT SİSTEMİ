public class Main{
public static void main( String []args){
Scanner k= new Scanner( System.in);
int x,y;
x=k.nextInt();
y=nextInt();
if(x>0 && y>0){
System.out.println(" 1. bölgededir. ");
}
else if(x<0 && y>0){
System.out.println(" 2. bölgededir.);
}
else if ( x<0 && y<0){
System.out.println( "3. bölgededir."); 
}
else if(x>0 &&y<0){
System.out.println(4. bölgededir." );
}
else
System.out.println(" orjindedir. ");
}
}
 
 // klavyeden girilen bir(x,y) noktasının bulunduğu bölgeyi bulan program
